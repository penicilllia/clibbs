
all: mainfl
mainfl: mainfile.o libmy1.a libmy2.so 
	gcc -o mainfl *.o libmy*
	export LD_LIBRARY_PATH='./'
	./mainfl
mainfile.o: mainfile.c
	gcc -c mainfile.c

libmy1.a: file1.o
	gcc -c file1.c
	ar r libmy1.a

libmy2.so: file2.o
	gcc -c -fPIC file2.c
	gcc -shared -o libmy.so file2.o
clean:
	rm -f *.o
